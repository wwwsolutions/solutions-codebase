{
  "version": 1,
  "projects": {
    "shared-styles": {
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/styles/**/*.ts"
            ]
          }
        }
      }
    },
    "shared-assets": {
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/assets/**/*.ts"
            ]
          }
        }
      }
    },
    "todomongoapp": {
      "root": "apps/todomongoapp",
      "sourceRoot": "apps/todomongoapp/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/todomongoapp",
            "index": "apps/todomongoapp/src/index.html",
            "main": "apps/todomongoapp/src/main.ts",
            "polyfills": "apps/todomongoapp/src/polyfills.ts",
            "tsConfig": "apps/todomongoapp/tsconfig.app.json",
            "assets": [
              "apps/todomongoapp/src/favicon.ico",
              "apps/todomongoapp/src/assets"
            ],
            "styles": [
              "apps/todomongoapp/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todomongoapp/src/environments/environment.ts",
                  "with": "apps/todomongoapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "todomongoapp:build",
            "proxyConfig": "apps/todomongoapp/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "todomongoapp:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todomongoapp/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todomongoapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todomongoapp-e2e": {
      "root": "apps/todomongoapp-e2e",
      "sourceRoot": "apps/todomongoapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/todomongoapp-e2e/cypress.json",
            "tsConfig": "apps/todomongoapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "todomongoapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "todomongoapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todomongoapp-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "todomongoapi": {
      "root": "apps/todomongoapi",
      "sourceRoot": "apps/todomongoapi/src",
      "projectType": "application",
      "prefix": "todomongoapi",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/todomongoapi",
            "main": "apps/todomongoapi/src/main.ts",
            "tsConfig": "apps/todomongoapi/tsconfig.app.json",
            "assets": [
              "apps/todomongoapi/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/todomongoapi/src/environments/environment.ts",
                  "with": "apps/todomongoapi/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "todomongoapi:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/todomongoapi/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/todomongoapi/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todomongoapi-controllers": {
      "root": "libs/todomongoapi/controllers",
      "sourceRoot": "libs/todomongoapi/controllers/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/todomongoapi/controllers/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/todomongoapi/controllers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todomongoapi-modules": {
      "root": "libs/todomongoapi/modules",
      "sourceRoot": "libs/todomongoapi/modules/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/todomongoapi/modules/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/todomongoapi/modules/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "todomongoapi-routes": {
      "root": "libs/todomongoapi/routes",
      "sourceRoot": "libs/todomongoapi/routes/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/todomongoapi/routes/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/todomongoapi/routes/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-data-access-models": {
      "root": "libs/shared/data-access-models",
      "sourceRoot": "libs/shared/data-access-models/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/data-access-models/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/data-access-models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "defaultProject": "todomongoapp"
}